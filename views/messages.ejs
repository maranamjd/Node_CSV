<div class="container-fluid">
  <div class="row">
    <div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-10">
      <table class="table table-striped table-bordered">
        <thead>
          <th>Name</th>
          <th>Email</th>
          <th>Message</th>
          <th>Action</th>
        </thead>
        <tbody>
            <% if(locals.messages){ %>
              <% for(var message in messages){ %>
                <tr>
                  <td><%= messages[message].name %></td>
                  <td><%= messages[message].email %></td>
                  <td><%= messages[message].message %></td>
                  <td><span class="btn btn-warning glyphicon glyphicon-list"> View</span></td>
                </tr>
              <% } %>
            <% }else{ %>
              No data....
            <% } %>
        </tbody>
      </table>
    </div>
    <div class="col-md-2 col-sm-1 col-xs-1">

    </div>
  </div>
</div>
